#include <iostream>

using namespace std;
long long nod_mine(long long a, long long b){
    while (a!=b){
        if (a>b){
            a=a-b;
        }
        else{
            b=b-a;
        }
    }
    return a; // Можно  и b возвращать
}
long long for_four(long long a,long long b, long long c, long long d){
    return nod_mine(nod_mine(nod_mine(a,b),c),d);
}
int main() {
    setlocale(LC_ALL,"Russian"); //импорт русской библиотеки(необезательно)
    long long a,b,c,d;//объявление переменной
    cout<<"Введите 4 числа для НОД \n";
    cin >> a;
    cin >> b;
    cin >> c;
    cin >> d;
    cout << "НОД("<< a << ','<< b << ',' << c << ','<< d <<")="<< for_four(a,b,c,d) <<".\n";
    return 0;

}
